<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:drawable="@drawable/btn_comp_enable">
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
        </shape>
    </item>

    <item android:state_enabled="true" android:drawable="@drawable/btn_comp_not_enable" >
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
        </shape>
    </item>

</selector>